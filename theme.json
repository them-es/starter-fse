{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": ["page", "post"]
		},
		{
			"name": "page-notitle",
			"title": "Page: No title",
			"postTypes": ["page"]
		}
	],
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "query",
			"title": "Query (The Loop)"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"gradients": [
				{
					"slug": "green-to-yellow",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--green), var(--wp--preset--color--yellow))",
					"name": "Green to Yellow"
				},
				{
					"slug": "yellow-to-green",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--yellow), var(--wp--preset--color--green))",
					"name": "Yellow to Green"
				},
				{
					"slug": "red-to-yellow",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--red), var(--wp--preset--color--yellow))",
					"name": "Red to Yellow"
				},
				{
					"slug": "yellow-to-red",
					"gradient": "linear-gradient(160deg, var(--wp--preset--color--yellow), var(--wp--preset--color--red))",
					"name": "Yellow to Red"
				}
			],
			"link": true,
			"palette": [
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				},
				{
					"name": "Dark-Gray",
					"slug": "dark-gray",
					"color": "#212529"
				},
				{
					"name": "Gray",
					"slug": "gray",
					"color": "#ABB8C3"
				},
				{
					"name": "Charcoal",
					"slug": "charcoal",
					"color": "#264653"
				},
				{
					"name": "Green",
					"slug": "green",
					"color": "#2A9D8F"
				},
				{
					"name": "Yellow",
					"slug": "yellow",
					"color": "#E9C46A"
				},
				{
					"name": "Orange",
					"slug": "orange",
					"color": "#F4A261"
				},
				{
					"name": "Red",
					"slug": "red",
					"color": "#E76F51"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#FFFFFF"
				},
				{
					"name": "Base",
					"slug": "base",
					"color": "var(--wp--preset--color--white)"
				},
				{
					"name": "Primary",
					"slug": "primary",
					"color": "var(--wp--preset--color--black)"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "var(--wp--preset--color--dark-gray)"
				},
				{
					"name": "Foreground",
					"slug": "foreground",
					"color": "var(--wp--preset--color--black)"
				},
				{
					"name": "Background",
					"slug": "background",
					"color": "var(--wp--preset--color--white)"
				}
			]
		},
		"layout": {
			"contentSize": "650px",
			"wideSize": "1200px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "1",
					"size": "1rem",
					"slug": "30"
				},
				{
					"name": "2",
					"size": "1.5rem",
					"slug": "40"
				},
				{
					"name": "3",
					"size": "1.75rem",
					"slug": "50"
				},
				{
					"name": "4",
					"size": "2rem",
					"slug": "60"
				},
				{
					"name": "5",
					"size": "3rem",
					"slug": "70"
				},
				{
					"name": "6",
					"size": "5rem",
					"slug": "80"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"lineHeight": true,
			"fontFamilies": [
				{
					"fontFamily": "system-ui, -apple-system, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, \"Noto Sans\", \"Liberation Sans\", sans-serif",
					"name": "Sans Serif",
					"slug": "sans-serif"
				},
				{
					"fontFamily": "Cambria, Georgia, \"Times New Roman\", Times, serif",
					"name": "Serif",
					"slug": "serif"
				},
				{
					"fontFamily": "SFMono-Regular, Menlo, Monaco, Consolas, \"Liberation Mono\", \"Courier New\", monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"size": "1rem",
					"slug": "small"
				},
				{
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"size": "clamp(1.75rem, 3vw, 2.25rem)",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true,
		"custom": {
			"typography": {
				"line-height": {
					"body": 1.7,
					"heading": 1.3,
					"page-title": 1.1
				},
				"font-weight": {
					"light": "300",
					"normal": "400"
				},
				"font-size": {
					"huge": "clamp(2.25rem, 4vw, 2.75rem)",
					"gigantic": "clamp(2.75rem, 6vw, 3.25rem)",
					"colossal": "clamp(3.25rem, 8vw, 6.25rem)"
				}
			}
		}
	},
	"styles": {
		"blocks": {
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "1.4"
				}
			},
			"core/search": {
				"border": {
					"width": "1px",
					"radius": "0px"
				},
				"color": {
					"text": "var(--wp--preset--color--dark-gray)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--20)",
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--20)",
						"right": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--body)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)",
					"textTransform": "uppercase"
				}
			},
			"core/code": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)"
					}
				},
				"border": {
					"width": "1.5px",
					"radius": "0px",
					"color": "var(--wp--preset--color--dark-gray)",
					"style": "solid"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--dark-gray)"
		},
		"elements": {
			"button": {
				"border": {
					"width": "1px",
					"color": "transparent",
					"style": "solid",
					"radius": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--dark-gray)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--gray)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--gray)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--gray)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":visited": {
					"color": {
						"background": "var(--wp--preset--color--gray)",
						"text": "var(--wp--preset--color--white)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--dark-gray)"
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--custom--typography--font-size--colossal)",
					"lineHeight": "var(--wp--custom--typography--line-height--page-title)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--custom--typography--font-size--huge)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "var(--wp--custom--typography--font-weight--normal)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"top": "var(--wp--preset--spacing--30)",
				"right": "var(--wp--preset--spacing--40)",
				"bottom": "var(--wp--preset--spacing--30)",
				"left": "var(--wp--preset--spacing--40)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		}
	}
}
